{
  "name": "server",
  "module": "index.ts",
  "type": "commonjs",
  "devDependencies": {
    "@types/bun": "latest",
    "nodemon": "^3.1.7",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@sequelize/core": "^7.0.0-alpha.42",
    "@sequelize/mysql": "^7.0.0-alpha.43",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.3",
    "path": "^0.12.7",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:prod": "rm -rf dist && tsc && cp .env.prod .env",
    "start": "node -r ts-node/register src/index.ts",
    "start:watch": "nodemon --watch 'src/**/*.ts' -e ts --exec node -r ts-node/register src/index.ts",
    "docker:build": "docker build -t simplycord-server .",
    "docker:run": "docker run -p 3000:3000 simplycord-server",
    "docker:stop": "docker stop simplycord-server",
    "docker:rm": "docker rm simplycord-server"
  }
}